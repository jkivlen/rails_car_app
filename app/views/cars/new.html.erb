<h1>Cars#new</h1>
<p>Find me in app/views/cars/new.html.erb</p>


<%= form_for(@car) do |f| %>
    <%= f.label :make %>
    <%= f.text_field :make %><br>
    <%= f.label :model %>
    <%= f.text_field :model %><br>
    <%= f.label :color %>
    <%= f.color_field :color %><br>
    <%= f.label :price %>
    <%= f.number_field :price, min: 0, step: 0.01 %><br>
    <%= f.submit %>
<% end %>